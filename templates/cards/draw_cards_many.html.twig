{% extends "base/base.html.twig" %}

{% block title %}Drawn Card{% endblock %}

{% block body %}
    <h1 class="center-text">Draget kort!</h1>
    <section>
        <h4 class="center-text">Det finns {{ cardRemaning }} kvar i leken.</h4>
        <div class="center-div">
            {% for card in cards %}
                <img src="{{ asset('img/'~ card ~'-min.svg') }}" alt="">
            {% endfor %}
        </div>
    </section>
{% endblock %}